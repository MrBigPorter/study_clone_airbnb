{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "es2022",
    "paths": {
      "@/*": [
        "./*",
        "./app/*",
        "./assets/*"
      ]
    },
  }, 
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
